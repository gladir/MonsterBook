Procedure PutWall(X,Y:Byte);Begin
 Dec(X);
 PutFillBox(X shl 4,Y*12,X shl 4+15,Y*12+11,Magenta);
 ClrLnHor(X shl 4,Y*12,4,LightGray);
 ClrLnHor(X shl 4+6,Y*12+1,7,LightGray);
 ClrLnHor(X shl 4,Y*12+2,2,LightGray);
 ClrLnHor(X shl 4+11,Y*12+3,4,LightGray);
 ClrLnHor(X shl 4+2,Y*12+4,3,LightGray);
 ClrLnHor(X shl 4+9,Y*12+6,4,LightGray);
 ClrLnHor(X shl 4+1,Y*12+6,5,LightGray);
 ClrLnHor(X shl 4+4,Y*12+8,6,LightGray);
 ClrLnHor(X shl 4+11,Y*12+9,4,LightGray);
 ClrLnHor(X shl 4+3,Y*12+10,3,LightGray);
End;

Procedure PutBrick(X,Y:Byte);Begin
 Dec(X);
 PutFillBox(X shl 4,Y*12,X shl 4+15,Y*12+11,Cyan);
 PutRect(X shl 4,Y*12,X shl 4+15,Y*12+11,Black);
 PutLn(X shl 4+1,Y*12,X shl 4+15,Y*12,White);
 PutLn(X shl 4,Y*12+1,X shl 4,Y*12+10,White);
 SetPixel(X shl 4,Y*12,LightGray);
 SetPixel(X shl 4+15,Y*12,DarkGray);
 SetPixel(X shl 4,Y*12+11,LightGray);
 SetPixel(X shl 4+15,Y*12+11,DarkGray);
End;

Procedure PutBox(X,Y:Byte);Begin
 Dec(X);
 PutFillBox(X shl 4,Y*12,X shl 4+15,Y*12+11,Brown);
 PutLn(X shl 4+15-2,Y*12+2,X shl 4+2,Y*12+11-2,DarkGray);
 PutLn(X shl 4+15-2-3,Y*12+2,X shl 4+2,Y*12+11-2-2,Black);
 PutLn(X shl 4+15-2-6,Y*12+2,X shl 4+2,Y*12+11-2-4,DarkGray);
 PutLn(X shl 4+15-2,Y*12+2+2,X shl 4+2+3,Y*12+11-2,Black);
 PutLn(X shl 4+15-2,Y*12+2+4,X shl 4+2+6,Y*12+11-2,DarkGray);
 PutRect(X shl 4+2,Y*12+2,X shl 4+15-2,Y*12+11-2,Black);
End;

