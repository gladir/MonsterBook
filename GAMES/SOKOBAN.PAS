Procedure PutName;Var I,J,K,Masque,P:Byte;S:String;Mtx:^TByte;Begin
 SetVideoModeDeluxe(vmGrf320x200c256);
 Mtx:=@MtxSokoBan;P:=0;
 For I:=0to 15do For J:=0to 2do Begin
  Masque:=Mtx^[P];
  For K:=0to 7do Begin
   If Masque and 128<>0Then PutBox(1+J*7+K,I);
   Masque:=Masque shl 1;
  End;
  Inc(P)
 End;
 ReadKey;
End;

Procedure PutFixe(X,Y:Byte);Begin
 Dec(X);
 PutFillBox(X shl 4,Y*12,X shl 4+15,Y*12+11,DarkGray);
 PutRect(X shl 4,Y*12,X shl 4+15,Y*12+11,Black);
 PutLn(X shl 4+1,Y*12,X shl 4+15,Y*12,White);
 PutLn(X shl 4,Y*12+1,X shl 4,Y*12+10,White);
 SetPixel(X shl 4,Y*12,LightGray);
 SetPixel(X shl 4+15,Y*12,DarkGray);
 SetPixel(X shl 4,Y*12+11,LightGray);
 SetPixel(X shl 4+15,Y*12+11,DarkGray);
End;

Procedure UnSetMan(X,Y:Byte);Begin
 Case Tab[0][Y][X]of
  '#':PutWall(X,Y);
  ' ':PutBrick(X,Y);
  '$':PutBox(X,Y);
  '.':PutFixe(X,Y);
 End;
End;

Procedure PutMove;Begin
 If(Run)Then PutTxtXY(34,MaxYTxts,WordToStr(RunP),$FC)
        Else PutTxtXY(34,MaxYTxts,WordToStr(NmMove),$FC);
End;

Procedure PutX(X,Y:Integer);Var I,J:Integer;Begin
 PutSprite(X,Y,X+15,Y+11,Mario);
{ For J:=0to 11do For I:=0to 15do Begin
  If Mario[J,I]<>8Then SetPixel(X+I,Y+J,Mario[J,I]);
 End;}
End;

Procedure SetMan(X,Y:Byte);Begin
 Case Tab[1][Y][X]of
  '#':PutWall(X,Y);
  ' ':PutBrick(X,Y);
  '$':PutBox(X,Y);
  '.':PutFixe(X,Y);
 End;
 Dec(X);
 PutX(X shl 4,Y*12);
 If Not(Run)Then Inc(NmMove);
 PutMove;
End;

Procedure UnPutBar;Var X,Y:Byte;Begin
 X:=(Stage-1)div 17;Y:=(Stage-1)mod 17;
 BarSelHor(X*8,Y+3,X*8+7,LightCyan);
End;

Procedure PutBar;Var X,Y:Byte;Begin
 X:=(Stage-1)div 17;Y:=(Stage-1)mod 17;
 BarSelHor(X*8,Y+3,X*8+7,LightCyan shl 4);
End;

Function Max(N,M:Byte):Byte;Begin
 If(N<M)Then Max:=N+1 Else Max:=1;
End;

Function Min(N,Max:Byte):Byte;Begin
 If N>1Then Min:=N-1 Else Min:=Max;
End;

Procedure ChoiceLevel;Var J:Byte;K:Wd;Begin
 SetVideoMode(vmTxtC40);
 SetBlink(No);
 CloseCur;
 PutTxtCenter(0,__Justified__,'Quel tage?',Yellow);
 SetKr(LightCyan);
 For J:=0to 16do Begin
  _PutTxtXY(3+0,3+J,WordToStr(J+1));
  _PutTxtXY(3+8,3+J,WordToStr(J+1+17));
  _PutTxtXY(3+16,3+J,WordToStr(J+1+17+17));
  _PutTxtXY(3+24,3+J,WordToStr(J+1+17+17+17));
  _PutTxtXY(3+32,3+J,WordToStr(J+1+17+17+17+17));
 End;
 PutBar;
 Repeat
  K:=ReadKey;
  Case(K)of
   kbLeft:Begin
    UnPutBar;
    Stage:=Min(Stage-16,85);
    PutBar;
   End;
   kbRight:Begin
    UnPutBar;
    Stage:=Max(Stage+16,85);
    PutBar;
   End;
   kbUp:Begin
    UnPutBar;
    Stage:=Min(Stage,85);
    PutBar;
   End;
   kbDn:Begin
    UnPutBar;
    Stage:=Max(Stage,85);
    PutBar;
   End;
   kbEnter:Break;
  End;
 Until No;
End;

Procedure MemKey(K:Wd);Begin
 If Not(Run)Then Begin
  MKey[PosKey]:=K;PosKey:=(PosKey+1)and 2047;
 End;
End;

Procedure Play;Label Restart;Var I,J,K:Wd;Chk:Bool;Begin
 SetVideoModeDeluxe(vmGrf320x200c256);
 Repeat
Restart:
  PosKey:=0;Run:=No;
  SetBckScr;
  ClrScr(Green shl 4);
  ClrLn(MaxYTxts,' ',$F0);
  PutTxtXY(2,MaxYTxts,'tage:',$F0);
  PutTxtXY(9,MaxYTxts,WordToStr(Stage),$FC);
  CurrPtr:=TP[Stage];NmMove:=$FFFF;
  NmElem:=0;LenElem:=StrLen(CurrPtr^[0]);
  FillClr(Tab,SizeOf(Tab));
  While(CurrPtr^[NmElem]<>NIL)do Begin
   Tab[0][NmElem]:=StrPas(CurrPtr^[NmElem]);Tab[1][NmElem]:=StrPas(CurrPtr^[NmElem]);Inc(NmElem)
  End;
  FixeOk:=0;NmFixe:=0;
  For J:=0to NmElem-1do For I:=1to(LenElem)do Begin
   Case Tab[0][J][I]of
    '#':PutWall(I,J);
    ' ':PutBrick(I,J);
    '$':Begin;PutBox(I,J);Tab[1][J][I]:=' ';End;
    '*':Begin;PutBox(I,J);Tab[0][J][I]:='$';Tab[1][J][I]:='.';Inc(FixeOk)End;
    '.':Begin;PutFixe(I,J);Inc(NmFixe)End;
    '@':Begin;XMan:=I;YMan:=J;Tab[0][J][I]:=' ';Tab[1][J][I]:=' ';SetMan(I,J)End;
   End;
  End;
  PutTxtXY(14,MaxYTxts,'Nombre dplacement:',$F0);
  PutMove;
  SetVisualScr;
  CopyBck2Visual;
  Repeat
   If(Run)Then Begin
    K:=MKey[RunP];Inc(RunP);
    If(KeyPress)or(RunP>=PosKey)Then Begin;Run:=No;NmMove:=RunP;End;
    WaitRetrace;WaitRetrace;WaitRetrace;WaitRetrace;
   End
    Else
   K:=ReadKey;
   Case(K)of
    kbF10:Goto Restart;
    kbF9:If Stage<85Then Begin;Inc(Stage);Goto Restart;End;
    kbF8:If Stage>1Then Begin;Dec(Stage);Goto Restart;End;
    kbF3:If FileExist('SOKOKEY.'+Str0(Stage,3))Then Begin
     PosKey:=GetFileSize('SOKOKEY.'+Str0(Stage,3))shr 1;RunP:=0;Run:=Ya;
     GetFile('SOKOKEY.'+Str0(Stage,3),0,PosKey shl 1,MKey);
    End;
    kbUp:Case Tab[0][YMan-1][XMan]of
     ' ','.':Begin
      UnSetMan(XMan,YMan);
      Dec(YMan);
      SetMan(XMan,YMan);
      MemKey(K);
     End;
     '$':If Tab[0][YMan-2][XMan]in[' ','.']Then Begin
      Chk:=(Tab[1][YMan-1][XMan]=' ')and(Tab[1][YMan-2][XMan]='.');
      If(Chk)Then Inc(FixeOk);
      Chk:=(Tab[1][YMan-1][XMan]='.')and(Tab[1][YMan-2][XMan]=' ');
      If(Chk)Then Dec(FixeOk);
      Tab[0][YMan-2][XMan]:=Tab[0][YMan-1][XMan];
      Tab[0][YMan-1][XMan]:=Tab[1][YMan-1][XMan];
      UnSetMan(XMan,YMan-2);
      UnSetMan(XMan,YMan);
      Dec(YMan);
      SetMan(XMan,YMan);
      MemKey(K);
      If(FixeOk=NmFixe)Then Break;
     End;
    End;
    kbDn:Case Tab[0][YMan+1][XMan]of
     ' ','.':Begin
      UnSetMan(XMan,YMan);
      Inc(YMan);
      SetMan(XMan,YMan);
      MemKey(K);
     End;
     '$':If Tab[0][YMan+2][XMan]in[' ','.']Then Begin
      Chk:=(Tab[1][YMan+1][XMan]=' ')and(Tab[1][YMan+2][XMan]='.');
      If(Chk)Then Inc(FixeOk);
      Chk:=(Tab[1][YMan+1][XMan]='.')and(Tab[1][YMan+2][XMan]=' ');
      If(Chk)Then Dec(FixeOk);
      Tab[0][YMan+2][XMan]:=Tab[0][YMan+1][XMan];
      Tab[0][YMan+1][XMan]:=Tab[1][YMan+1][XMan];
      UnSetMan(XMan,YMan+2);
      UnSetMan(XMan,YMan);
      Inc(YMan);
      SetMan(XMan,YMan);
      MemKey(K);
      If(FixeOk=NmFixe)Then Break;
     End;
    End;
    kbLeft:Case Tab[0][YMan][XMan-1]of
     ' ','.':Begin
      UnSetMan(XMan,YMan);
      Dec(XMan);
      SetMan(XMan,YMan);
      MemKey(K);
     End;
     '$':If Tab[0][YMan][XMan-2]in[' ','.']Then Begin
      Chk:=(Tab[1][YMan][XMan-1]=' ')and(Tab[1][YMan][XMan-2]='.');
      If(Chk)Then Inc(FixeOk);
      Chk:=(Tab[1][YMan][XMan-1]='.')and(Tab[1][YMan][XMan-2]=' ');
      If(Chk)Then Dec(FixeOk);
      Tab[0][YMan][XMan-2]:=Tab[0][YMan][XMan-1];
      Tab[0][YMan][XMan-1]:=Tab[1][YMan][XMan-1];
      UnSetMan(XMan-2,YMan);
      UnSetMan(XMan,YMan);
      Dec(XMan);
      SetMan(XMan,YMan);
      MemKey(K);
      If(FixeOk=NmFixe)Then Break;
     End;
    End;
    kbRight:Case Tab[0][YMan][XMan+1]of
     ' ','.':Begin
      UnSetMan(XMan,YMan);
      Inc(XMan);
      SetMan(XMan,YMan);
      MemKey(K);
     End;
     '$':If Tab[0][YMan][XMan+2]in[' ','.']Then Begin
      Chk:=(Tab[1][YMan][XMan+1]=' ')and(Tab[1][YMan][XMan+2]='.');
      If(Chk)Then Inc(FixeOk);
      Chk:=(Tab[1][YMan][XMan+1]='.')and(Tab[1][YMan][XMan+2]=' ');
      If(Chk)Then Dec(FixeOk);
      Tab[0][YMan][XMan+2]:=Tab[0][YMan][XMan+1];
      Tab[0][YMan][XMan+1]:=Tab[1][YMan][XMan+1];
      UnSetMan(XMan+2,YMan);
      UnSetMan(XMan,YMan);
      Inc(XMan);
      SetMan(XMan,YMan);
      MemKey(K);
      If(FixeOk=NmFixe)Then Break;
     End;
    End;
    kbEsc:Break;
   End;
  Until No;
  If(K<>kbEsc)Then Begin
   ClrLn(MaxYTxts-1,' ',$F0);
   ClrLn(MaxYTxts,' ',$F0);
   PutTxtCenter(MaxYTxts-1,__Justified__,'tage'+BasicStrW(Stage)+' fait!!!',$F0);
   PutTxtCenter(MaxYTxts,__Justified__,'Pse une touche pour continuer',$F0);
   ClrKbd;
   If(ReadKey=kbF2)Then Begin
    SetFile('SOKOKEY.'+Str0(Stage,3),0,PosKey shl 1,MKey);
   End;
   Inc(Stage);
   If Stage=86Then Begin
    ClrScrBlack;
    PutTxtCenter(12,__Justified__,'Tous les tages sont fait!!!',$F);
    ClrLn(MaxYTxts,' ',$F0);
    PutTxtCenter(MaxYTxts,__Justified__,'Presse une touche pour quitter',$F0);
    ReadKey;
    Break;
   End;
  End;
 Until K=kbEsc;
End;

BEGIN
 InitSystems(suIsabel);
 WorldAnim;
 PutName;
 BackgroundScreen:=MemNew(64032);
 If(BackgroundScreen=NIL)Then Begin;WriteLn('Manque de mmoire!');Halt;End;
 Inc(PtrRec(BackgroundScreen).Seg);PtrRec(BackgroundScreen).Ofs:=0;
 Stage:=1;
 ChoiceLevel;
 Play;
 DoneLuxeVideo;
END.